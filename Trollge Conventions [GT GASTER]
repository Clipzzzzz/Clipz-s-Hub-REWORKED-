tab:Toggle("Auto Delete Common Chests",false, function(t)
	print(t)
	getgenv().adcc = t

	local commontable = {
		["Chest"] = "Chest",
		["Dark Chest"] = "Dark Chest",
		["Fish Chest"] = "Fish Chest"
	}

	while getgenv().adcc do
		wait(0.05)
		for i,v in game.Players.LocalPlayer.Backpack:GetChildren() do
			wait(0.05)
			if v:IsA "Tool" then
				for i,z in commontable do
					if v.Name == z then
						local tool = v
						tool:Destroy()
					end
				end
			end
		end
	end
end)

