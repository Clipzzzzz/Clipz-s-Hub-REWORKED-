function createtele(name, pos)
	local zonepart = Instance.new("Part")
	zonepart.Name = name
	zonepart.Parent = workspace
	zonepart.Anchored = true
	zonepart.Position = pos
	zonepart.Transparency = 1
end

tab3:Button("Arena", function()
	local zonename = "PC_Arena"
	local position = Vector3.new(-85.92035675048828, -42.6500129699707, -91.29226684570312)

	if game.Workspace:FindFirstChild(zonename) == nil then
		createtele(zonename, position)
		wait(0.5)
		game:GetService "Players".LocalPlayer.Character:FindFirstChild "HumanoidRootPart".CFrame = game.Workspace:FindFirstChild(zonename).CFrame	
	else
		game.Workspace:FindFirstChild(zonename):Destroy()
		createtele(zonename, position)
		wait(0.5)
		game:GetService "Players".LocalPlayer.Character:FindFirstChild "HumanoidRootPart".CFrame = game.Workspace:FindFirstChild(zonename).CFrame	
	end	
end)

