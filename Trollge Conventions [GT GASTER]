--tab:Label("Label")

tab4:Toggle("Chest ESP",false, function(t)
	print(t)

	local chests = game.Workspace.chests:GetChildren()
	local RunService = game:GetService("RunService")
	local hightlight = Instance.new("Highlight")
	hightlight.Name = "Highlight"

	for i, v in pairs(chests) do
		if v:FindFirstChild("Highlight") == nil and t == true then
			local highlightclone = hightlight:Clone()
			highlightclone.Parent = v
			highlightclone.FillColor = Color3.fromRGB(0, 0, 0)
			highlightclone.FillTransparency = 0.7
			highlightclone.DepthMode = Enum.HighlightDepthMode.AlwaysOnTop
			highlightclone.Name = "Highlight"
		else
			v:FindFirstChild("Highlight"):Destroy()
		end
	end

	game.Workspace.chests.ChildAdded:Connect(function()
		if not chests:FindFirstChild("Highlight") and t == true then
			local highlightclone = hightlight:Clone()
			hightlight.Parent = chests
			highlightclone.FillColor = Color3.fromRGB(0, 0, 0)
			highlightclone.FillTransparency = 0.7
			highlightclone.DepthMode = Enum.HighlightDepthMode.AlwaysOnTop
			highlightclone.Name = "Highlight"
		end
	end)

	game.Workspace.chests.ChildRemoved:Connect(function(chest)
		chest:FindFirstChild("Highlight"):Destroy()
	end)

	if t == false then
		for i, v in pairs(chests) do
			if v:FindFirstChild("Highlight") ~= nil then
				v:FindFirstChild("Highlight"):Destroy()
			end
		end
	end
end)

tab4:Toggle("Anti AFK",false, function(t)
	print(t)
	getgenv().anti = t

	local VU = game:GetService("VirtualUser")
	game.Players.LocalPlayer.Idled:Connect(function()
		if getgenv().anti then
			VU:CaptureController()VU:ClickButton2(Vector2.new())
		end
	end)
end)

local gooditems = {"Shadow Cup", "Nightmare Cup", "THE HAPPINESS", "War Blood", "Mysterious Door", "Monarch", "Voices", "Ruler Cup", "Blood Cup"}

tab4:Toggle("Item Alerts (UI Notification)",false, function(t)
	print(t)
	getgenv().alertui = t

	while getgenv().alertui do
		wait(0.05)
		for i,v in game:GetService("Players").LocalPlayer.Backpack:GetChildren() do
			if getgenv().alertui == false then return end
				if table.find(gooditems, v.Name) and v:FindFirstChild("SeenA") == nil then
					local tag = Instance.new("BoolValue")
					tag.Name = "SeenA"
					tag.Parent = v
					lib:Notification("Project Clipz [Rare Item Alert]", "You obtained "..v.Name, "Noted")	
					local sound = Instance.new("Sound")
					sound.Parent = workspace
				    sound.SoundId = 333638961
				    sound.Volume = 1
				sound:Play()
				sound.Ended:Wait()
				sound:Destroy()
			end
		end
	end
end)

local webhook = ""

local function alertdiscord(item)
		local url = webhook
		local data = {
			["content"] = "<@everyone>",
			["embeds"] = {
				{
					["title"] = "**Project Clipz [Rare Item Alert]**",
					["description"] = "You obtained "..tostring(item),
					["type"] = "rich",
					["color"] = tonumber(0),
					["image"] = {
						["url"] = "http://www.roblox.com/Thumbs/Avatar.ashx?x=150&y=150&Format=Png&username=" ..
							tostring(game:GetService("Players").LocalPlayer.Name)
					}
				}
			}
		}
		local newdata = game:GetService("HttpService"):JSONEncode(data)

		local headers = {
			["content-type"] = "application/json"
		}

		local abcdef = {Url = url, Body = newdata, Method = "POST", Headers = headers}
		request(abcdef)
end

tab4:Toggle("Item Alerts (Discord Notification)",false, function(t)
	print(t)
	getgenv().alertdsc = t

	while getgenv().alertdsc do
		wait(0.05)
		for i,v in game:GetService("Players").LocalPlayer.Backpack:GetChildren() do
			if getgenv().alertdsc == false then return end
			if v:IsA "Tool" then
				if table.find(gooditems, v.Name) and v:FindFirstChild("SeenB") == nil then
					local tag = Instance.new("BoolValue")
					tag.Name = "SeenB"
					tag.Parent = v
					alertdiscord(v.Name)
				end
			end
		end
	end
end)

tab2:Textbox("Webhook",true, function(t)
	print(t)
	webhook = t
end)


tab4:Button("Rejoin", function()
	game:GetService("TeleportService"):Teleport(game.PlaceId)
end)

tab4:Button("Server Hop", function()
	local x = {}
	for _, v in ipairs(game:GetService("HttpService"):JSONDecode(game:HttpGetAsync("https://games.roblox.com/v1/games/" .. game.PlaceId .. "/servers/Public?sortOrder=Asc&limit=100")).data) do
		if type(v) == "table" and v.maxPlayers > v.playing and v.id ~= game.JobId then
			x[#x + 1] = v.id
		end
	end
	if #x > 0 then
		game:GetService("TeleportService"):TeleportToPlaceInstance(game.PlaceId, x[math.random(1, #x)])
	end
end)

tab4:Button("Destroy UI", function()
	game:GetService("CoreGui").ui:Destroy()
	lib:Notification("Project Clipz", "UI Destroyed, rexecute the loader to get it back.", "Noted")
end)

tab4:Colorpicker("Change UI Color",Color3.fromRGB(44, 120, 224), function(t)
	lib:ChangePresetColor(Color3.fromRGB(t.R * 255, t.G * 255, t.B * 255))
end)

