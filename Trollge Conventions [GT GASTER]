local lib = loadstring(game:HttpGet"https://raw.githubusercontent.com/dawid-scripts/UI-Libs/main/Vape.txt")()

local win = lib:Window("Project Clipz",Color3.fromRGB(44, 120, 224), Enum.KeyCode.RightControl)

local tab = win:Tab("Main")
local tab2 = win:Tab("Player")
local tab3 = win:Tab("Teleports")
local tab4 = win:Tab("Misc")

tab:Toggle("Auto Farm Chests",false, function(t)
	print(t)
	getgenv().afc = t
	while getgenv().afc do
		wait(0.05)
		for _, v in pairs(game:GetService("Workspace").chests:GetChildren()) do
			if v:IsA "Part" then
				v.Anchored = false
				game:GetService "Players".LocalPlayer.Character:FindFirstChild "HumanoidRootPart".CFrame = v.CFrame
			end
			local chestprox = v:FindFirstChildOfClass("ProximityPrompt")
			fireproximityprompt(chestprox)
			-- The function that takes place when the toggle is pressed
			-- The variable (Value) is a boolean on whether the toggle is true or false
		end
	end
end)

tab:Toggle("Auto Open Chests",false, function(t)
	print(t)
	getgenv().aoc = t

	local chests = {"Chest", "Dark Chest", "Light Chest", "Fish Chest", "Bone Chest", "Trevor Chest", "Love Chest", "Giant Fish Chest", "Ink Chest", "Undertale Chest", "Flesh Chest", "Earth Chest", "106 Chest", "096 Chest", "Troll 999 Chest"}


	while getgenv().aoc do
		wait(0.05)
		for _, v in pairs(game:GetService("Players").LocalPlayer.Backpack:GetChildren()) do
			wait(0.05)
			if getgenv().aoc == false then return end
			if table.find(chests, v.Name) then
					local tool = v
					tool.Parent = game.Players.LocalPlayer.Character
					wait(0.05)
					tool:Activate()
					wait(0.05)
					tool.Parent = game.Players.LocalPlayer.Backpack
			end
		end
	end
end)	

tab:Toggle("Safe Zone",false, function(t)
	print(t)
	getgenv().sz = t
	if getgenv().sz then
		if game.Workspace:FindFirstChild("PC_SafeZone") == nil then
			local safezonepart = Instance.new("Part")
			safezonepart.Name = "PC_SafeZone"
			safezonepart.Parent = workspace
			safezonepart.Anchored = true
			safezonepart.Position = Vector3.new(0, 100, 0)
			safezonepart.Size = Vector3.new(10, 1, 10)
			while getgenv().sz do
				wait(0.5)
				game:GetService "Players".LocalPlayer.Character:FindFirstChild "HumanoidRootPart".CFrame = safezonepart.CFrame + Vector3.new(safezonepart.Position.X, 5, safezonepart.Position.Z)
			end	
		else
			game.Workspace:FindFirstChild("PC_SafeZone"):Destroy()
			local safezonepart = Instance.new("Part")
			safezonepart.Name = "PC_SafeZone"
			safezonepart.Parent = workspace
			safezonepart.Anchored = true
			safezonepart.Position = Vector3.new(0, 100, 0)
			safezonepart.Size = Vector3.new(10, 1, 10)
			while getgenv().sz do
				wait(0.5)
				game:GetService "Players".LocalPlayer.Character:FindFirstChild "HumanoidRootPart".CFrame = safezonepart.CFrame + Vector3.new(safezonepart.Position.X, 5, safezonepart.Position.Z)
			end	
		end
	end
end)

tab:Toggle("Auto Delete Useless Items",false, function(t)
	print(t)
	getgenv().useless = t

	local items = {"Oil Cup", "Blood Cup", "Acid Cup", "Love Cup", "Light Cup", "Fish", "Mega Fish Bait"}


	while getgenv().useless do
		wait(0.05)
		for i,v in game:GetService("Players").LocalPlayer.Backpack:GetChildren() do
			if getgenv().useless == false then return end
			if v:IsA "Tool" then
				if table.find(items, v.Name) then
					local tool = v
					tool:Destroy()
				end
			end
		end
	end
end)

