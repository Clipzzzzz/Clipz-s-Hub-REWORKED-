tab3:Button("Secret Shop Room", function()
	local zonename = "PC_SecretShopRoom"
	local position = Vector3.new(1287.11083984375, -52.10249710083008, 130821.7109375)

	if game.Workspace:FindFirstChild(zonename) == nil then
		createtele(zonename, position)
		for i,v in game.Workspace.Shop.Room:GetChildren() do
			if v.Name == "Anti Cheat" then
				v:Destroy()
			end
		end
		wait(0.5)
		game:GetService "Players".LocalPlayer.Character:FindFirstChild "HumanoidRootPart".CFrame = game.Workspace:FindFirstChild(zonename).CFrame	
	else
		game.Workspace:FindFirstChild(zonename):Destroy()
		createtele(zonename, position)
		wait(0.5)
		game:GetService "Players".LocalPlayer.Character:FindFirstChild "HumanoidRootPart".CFrame = game.Workspace:FindFirstChild(zonename).CFrame	
	end	
end)


